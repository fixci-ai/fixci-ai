# Confluence Upload Configuration
# Copy this file to .env and fill in your details

# Your Confluence site URL (e.g., https://yourcompany.atlassian.net)
CONFLUENCE_SITE_URL=https://yoursite.atlassian.net

# Your Confluence email address
CONFLUENCE_EMAIL=your.email@example.com

# API token from https://id.atlassian.com/manage-profile/security/api-tokens
CONFLUENCE_API_TOKEN=your-api-token-here

# The space key where documentation will be created (e.g., FIXCI, DOCS, etc.)
CONFLUENCE_SPACE_KEY=FIXCI
